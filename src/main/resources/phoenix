CREATE TABLE IF NOT EXISTS "PA_STUDENT"(
  PK VARCHAR NOT NULL PRIMARY KEY,
  CF1.NAME VARCHAR,
  CF1:AGE VARCHAR,
  CF1:SEX VARCHAR,
  CF2:FATHER VARCHAR,
  CF2:MOTHER VARCHAR
);

CREATE VIEW "PA_STUDENT"(
  PK VARCHAR NOT NULL PRIMARY KEY,
  CF1.NAME VARCHAR,
  CF1:AGE VARCHAR,
  CF1:SEX VARCHAR,
  CF2:FATHER VARCHAR,
  CF2:MOTHER VARCHAR
) AS SELECT * FROM "PA_STUDENT";

UPSERT INTO "PA_STUDENT"(PK, NAME, AGE, SEX, FATHER, MOTHER) VALUES('001', 'zs', '18', '1', 'zsa', 'zsb');
